 int main()
 {
 	int a,key;
 	char b;
 	file *fnew;
 	char lastinput;
 	 while(true)
	  {
	  	welcome();
	  	system("cls");
	  	menu();
	  	ch=getch();
	  	char ch;
	  	swich(ch)
	  	{
	  		case'1';
	  		fileopen(ch);
	  		case'2';
	  		fileopen(ch);
	  		getch();
	  		case'3';
	  		system("cls");
	  		exit(0);
		  }
	   } 
	   
 }
