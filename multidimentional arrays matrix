#include <stdio.h>
int main()
{
    int i,j,m[3][3];
    for(i=0; i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("element [%d],[%d]",i,j);
            scanf("%d",&*(*m+j)+i);
        }
    }
    printf("matrix is");
    for(i=0; i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("%d",*(*m+i)+j);
        }
    }
}
